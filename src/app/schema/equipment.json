{
  "map": "equipment",
  "label": "Equipment",
  "fields": [{
    "map": "name",
    "type": "text",
    "label": "Name"
  }, {
    "map": "description",
    "type": "textarea",
    "label": "Description"
  }, {
    "map": "sensorType",
    "type": "select",
    "label": "Sensor type",
    "options": [{
      "label": "Radar",
      "value": "radar"
    },{
      "label": "Comint / Elint",
      "value": "elint"
    }]
  }, {
    "type": "tabs",
    "tabs": [{
      "label": "Transceiver & Antenna",
      "fields": [{
        "map": "transceiver.startFrequency",
        "type": "number",
        "label": "Start frequency [MHz]"
      }, {
        "map": "transceiver.stopFrequency",
        "type": "number",
        "label": "Stop frequency [MHz]"
      }, {
        "map": "transceiver.referenceFrequency",
        "type": "number",
        "label": "Reference frequency [MHz]"
      }, {
        "map": "transceiver.txPower",
        "type": "number",
        "label": "Tx power [Watt]"
      }, {
        "map": "transceiver.sensitivity",
        "type": "number",
        "label": "Sensitivity [dBm]"
      }, {
        "map": "transceiver.noiseFigure",
        "type": "number",
        "label": "Noise figure [dB]"
      }, {
        "map": "transceiver.bandWidth",
        "type": "number",
        "label": "Band width [kHz]"
      }, {
        "map": "transceiver.averageScanPeriod",
        "type": "number",
        "label": "Average scan period [s]"
      }, {
        "map": "transceiver.patternType",
        "type": "number",
        "label": "Pattern type"
      }, {
        "map": "transceiver.isOmnidirectionalAntenna",
        "type": "select",
        "label": "Is omnidirectional antenna",
        "options": [{
          "label": "True",
          "value": true
        }, {
          "label": "False",
          "value": false
        }]
      }, {
        "map": "transceiver.antennaPattern",
        "type": "select",
        "label": "Antenna pattern",
        "source": {
          "type": "antennaPattern",
          "key": "name"
        },
        "view": {
          "label": "View"
        }
      }, {
        "map": "transceiver.antennaGain",
        "type": "number",
        "label": "Antenna Gain [dB]"
      }]
    }, {
      "label": "Working Point",
      "disabled": "this.sensorType === 'elint'",
      "fields": [{
        "map": "workingPoint.rcsReference",
        "type": "number",
        "label": "RCS reference [m^2]"
      }, {
        "map": "workingPoint.rangeReference",
        "type": "number",
        "label": "Range reference [m]"
      }, {
        "map": "workingPoint.snrReference",
        "type": "number",
        "label": "SNR reference [dB]"
      }, {
        "map": "workingPoint.pfa",
        "type": "number",
        "label": "Pfa"
      }]
    }, {
      "label": "PD",
      "disabled": "this.sensorType === 'elint'",
      "fields": [{
        "map": "pd.pdAnalysisMode",
        "type": "select",
        "label": "PD analysis mode",
        "options": [{
          "label": "Digital",
          "value": "digital"
        }, {
          "label": "Analog",
          "value": "analog"
        }]
      }, {
        "map": "pd.isCoherentIntegration",
        "type": "select",
        "label": "Is coherent integration",
        "options": [{
          "label": "True",
          "value": true
        }, {
          "label": "False",
          "value": false
        }]
      }, {
        "map": "pd.angularDetectionSector",
        "type": "number",
        "label": "Angular detection sector [deg]"
      }, {
        "map": "pd.scanTime",
        "type": "number",
        "label": "Scan time (full sweep) [s]"
      }, {
        "map": "pd.numberOfPulsesInBatch",
        "type": "number",
        "label": "Number of pulses in batch"
      }, {
        "map": "pd.pri",
        "type": "number",
        "label": "PRI [s]"
      }, {
        "map": "pd.pulseDuration",
        "type": "number",
        "label": "Pulse duration [s]"
      }, {
        "map": "pd.isFreqPulseCompression",
        "type": "select",
        "label": "Is Freq pulse compression",
        "options": [{
          "label": "True",
          "value": true
        }, {
          "label": "False",
          "value": false
        }]
      }, {
        "map": "pd.pulseCompressionBW",
        "type": "number",
        "label": "Pulse compression BW [Hz]"
      }, {
        "map": "pd.isPhasePulseCompression",
        "type": "select",
        "label": "Is Phase pulse compression",
        "options": [{
          "label": "True",
          "value": true
        }, {
          "label": "False",
          "value": false
        }]
      }, {
        "map": "pd.pulseCompressionBinWidth",
        "type": "number",
        "label": "Pulse compression bin width [s]"
      }, {
        "map": "pd.lScansForCumulativePD",
        "type": "number",
        "label": "L - scans for cumulative PD"
      }]
    }, {
      "label": "Thresholds",
      "disabled": "this.sensorType === 'radar'",
      "fields": [{
        "map": "thresholds.minSnrDetectionFF",
        "type": "number",
        "label": "Min. SNR Detection FF [dB]"
      }, {
        "map": "thresholds.minSnrDetectionFH",
        "type": "number",
        "label": "Min. SNR Detection FH [dB]"
      }, {
        "map": "thresholds.minSnrDTOAFF",
        "type": "number",
        "label": "Min. SNR DTOA FF [dB]"
      }, {
        "map": "thresholds.minSnrDTOAFH",
        "type": "number",
        "label": "Min. SNR DTOA FH [dB"
      }, {
        "map": "thresholds.minSnrDDOPFF",
        "type": "number",
        "label": "Min. SNR DDOP FF [dB]"
      }, {
        "map": "thresholds.minSnrDDOPFh",
        "type": "number",
        "label": "Min. SNR DDOP FH [dB]"
      }, {
        "map": "thresholds.minSnrDOAFF",
        "type": "number",
        "label": "Min. SNR DOA FF [dB]"
      }, {
        "map": "thresholds.minSnrDOAFH",
        "type": "number",
        "label": "Min. SNR DOA FH [dB]"
      }, {
        "map": "thresholds.minSnrTOAFF",
        "type": "number",
        "label": "Min. SNR TOA FF [dB]:"
      }, {
        "map": "thresholds.minSnrTOAFH",
        "type": "number",
        "label": "Min. SNR TOA FH [dB]:"
      }]
    }, {
      "label": "Errors",
      "disabled": "this.sensorType === 'radar'",
      "fields": [{
        "map": "errors.frequencyError",
        "type": "number",
        "label": "Frequency error [Hz]"
      }, {
        "map": "errors.DTOAErrorBias",
        "type": "number",
        "label": "DTOA error - Bias [ns]"
      }, {
        "map": "errors.DTOAErrorSTD",
        "type": "number",
        "label": "DTOA error - STD [ns]"
      }, {
        "map": "errors.DDOPErrorBias",
        "type": "number",
        "label": "DDOP error - Bias [mHz]"
      }, {
        "map": "errors.DDOPErrorSTD",
        "type": "number",
        "label": "DDOP error – STD [mHz]"
      }, {
        "map": "errors.DOAErrorBias",
        "type": "number",
        "label": "DOA error – Bias [deg]"
      }, {
        "map": "errors.DOAErrorSTD",
        "type": "number",
        "label": "DOA error – STD [deg]"
      }, {
        "map": "errors.TOAErrorBias",
        "type": "number",
        "label": "TOA error – Bias [ns]"
      }, {
        "map": "errors.TOAErrorSTD",
        "type": "number",
        "label": "TOA error – STD [ns]"
      }]
    }]
  }]
}
